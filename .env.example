# .env.example — Copy this file to .env and fill in your values.
# The .env file is gitignored and must NEVER be committed.

# Test environment: dev | prod
TEST_ENV=dev

# Browser
BROWSER_TYPE=chromium
HEADLESS=false

# Application under test
BASE_URL=https://www.saucedemo.com/

# API base URL
API_BASE_URL=https://jsonplaceholder.typicode.com

# Test credentials — replace with real values in .env (never commit passwords)
TEST_STANDARD_USER=standard_user
TEST_STANDARD_PASSWORD=REPLACE_ME
TEST_LOCKED_USER=locked_out_user
TEST_PROBLEM_USER=problem_user
TEST_INVALID_USER=invalid_user
TEST_INVALID_PASSWORD=wrong_password

# Logging
LOG_LEVEL=INFO
